{
  "name": "angular-starter-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "testapi-win": "cd src/app/examples/test-api && start-server.bat",
    "testapi-mac": "cd src/app/examples/test-api && sh start-server.sh",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "moduleNameMapper": {
    "@store/(.*)": "src/app/store/$1",
    "@shared/(.*)": "src/app/shared/$1",
    "@carbonate/(.*)": "<rootDir>/node_modules/@signify/carbonate-components/dist/types/components/models/$1"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^14.3.0",
    "@angular/animations": "14.2.7",
    "@angular/cdk": "14.2.7",
    "@angular/common": "14.2.7",
    "@angular/compiler": "14.2.7",
    "@angular/core": "14.2.7",
    "@angular/forms": "14.2.7",
    "@angular/localize": "14.2.7",
    "@angular/platform-browser": "14.2.7",
    "@angular/platform-browser-dynamic": "14.2.7",
    "@angular/router": "14.2.7",
    "@ng-bootstrap/ng-bootstrap": "13.1.1",
    "@ngrx/effects": "14.2.0",
    "@ngrx/router-store": "14.2.0",
    "@ngrx/store": "14.2.0",
    "@ngrx/store-devtools": "14.2.0",
    "@okta/okta-angular": "6.0.0",
    "@okta/okta-auth-js": "7.0.0",
    "ag-grid-angular": "30.0.0",
    "ag-grid-community": "30.0.0",
    "ag-grid-enterprise": "30.0.0",
    "date-fns": "2.29.3",
    "rxjs": "7.5.7",
    "semantic-ui-css": "2.5.0",
    "tslib": "2.4.0",
    "zone.js": "0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.7",
    "@angular/cli": "14.2.6",
    "@angular/compiler-cli": "14.2.7",
    "@angular/language-service": "14.2.0",
    "@okta/okta-auth-js": "7.0.0",
    "@types/jasmine": "^4.0.3",
    "jasmine-core": "4.3.0",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "ng-packagr": "14.2.2",
    "ngx-build-plus": "14.0.0",
    "npm-run-all": "4.1.5",
    "typescript": "4.7.4"
  }
}
